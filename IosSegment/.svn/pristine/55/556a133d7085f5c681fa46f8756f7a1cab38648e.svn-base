<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="editTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bac_color_hui</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:drawableStart">@mipmap/search_icon</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:imeOptions">actionSearch</item>
        <item name="android:padding">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:hint">Search</item>
    </style>
    <style name="editTextStylebai">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bac_color_bai</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:drawableStart">@mipmap/search_icon</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:imeOptions">actionSearch</item>
        <item name="android:padding">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:hint">Search</item>
    </style>

    <style name="textViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">20sp</item>
    </style>
</resources>